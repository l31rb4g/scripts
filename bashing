#!/bin/bash

function out {
    echo -e '\e[1;'$1'm '$2'\e[m'
}
function info {
    out 34 ">>> $1"
}
function success {
    out 32 "âœ“ $1"
}
function warn {
    out 33 "[!] $1"
}
function error {
    out 31 "ERROR: $1"
}
function contains {
    has=0
    for needle in $1; do
        echo $needle;
    done
}
